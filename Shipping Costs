weight = 41.5
#Ground Shipping
if weight <= 2:
  cost_ground_shipping = 1.5 * weight +20
  print("Ground Shipping", cost_ground_shipping)
elif weight >= 2 and weight <= 6:
  cost_ground_shipping = 3 * weight +20
  print("Ground Shipping", cost_ground_shipping)
elif weight > 6 and weight <= 10:
  cost_ground_shipping = 4 * weight +20
  print("Ground Shipping", cost_ground_shipping) 
elif weight > 10:
  cost_ground_shipping = 4.75 * weight +20
  print("Ground Shipping", cost_ground_shipping)
# Premium Ground Shipping
premium_ground_shipping = 125.00
print("The price of premium ground shipping is", premium_ground_shipping)
# Drone Shipping
if weight <= 2:
  cost_drone_shipping = 4.5 * weight
  print("Drone shipping", cost_drone_shipping)
elif weight >= 2 and weight <= 6:
  cost_ground_shipping = 9 * weight
  print("Drone shipping", cost_ground_shipping)
elif weight > 6 and weight <= 10:
  cost_ground_shipping = 12 * weight
  print("Drone shipping", cost_ground_shipping) 
elif weight > 10:
  cost_ground_shipping = 14.25 * weight
  print("Drone shipping", cost_ground_shipping)
